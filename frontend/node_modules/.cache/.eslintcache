{\r\n         e.preventDefault();\r\n         try{\r\n            const { data } = await axios.put(\r\n                '/api/users/profile',\r\n                {name, email, password},\r\n                { headers: { Authorization: `Bearer ${userInfo.token}` } }\r\n              );\r\n              dispatch({\r\n                type:'UPDATE_SUCCESS',\r\n            });\r\n            ctxDispatch ({type:'USER_SIGNIN', payload:data});\r\n            localStorage.setItem('userInfo', JSON.stringify(data));\r\n            toast.success('User Update Successfully');\r\n         }catch(err){\r\n            dispatch({\r\n                type:'FETCH_FAIL',\r\n            });\r\n            toast.error(getError(err));\r\n         }\r\n    }\r\n  return (\r\n    <div className='container'>\r\n        <Helmet>\r\n            <title>User profile</title>\r\n        </Helmet>\r\n        <h1 className=\"my-3\">User profile</h1>\r\n        <div className=\"signinBox\">\r\n            <Form onSubmit= {submitHandler}>\r\n                <Form.Group className=\"mb-3\" controlId=\"name\">\r\n                    <Form.Label>Your Name</Form.Label>\r\n                    <Form.Control type=\"text\" onChange={(e) => setName(e.target.value)} value={name} required/> \r\n                </Form.Group>\r\n                <Form.Group className=\"mb-3\" controlId=\"email\">\r\n                    <Form.Label>Your Email</Form.Label>\r\n                    <Form.Control type=\"text\" onChange={(e) => setEmail(e.target.value)} value={email} required /> \r\n                </Form.Group>\r\n                <Form.Group className=\"mb-3\" controlId=\"password\">\r\n                    <Form.Label>Password</Form.Label>\r\n                    <Form.Control type=\"text\" onChange={(e) => setPassword(e.target.value)} /> \r\n                </Form.Group> \r\n                <Form.Group className=\"mb-3\" controlId=\"confirmPassword\">\r\n                    <Form.Label>Confirm Password</Form.Label>\r\n                    <Form.Control type=\"text\" onChange={(e) => setConfirmPassword(e.target.value)}  /> \r\n                </Form.Group>\r\n                <Form.Group className=\"mb-3\" controlId=\"confirmPassword\">     \r\n                    <Button variant=\"danger\" type=\"submit\">Update Profile </Button>\r\n                </Form.Group>\r\n            </Form>\r\n    </div>\r\n    </div>\r\n  )\r\n}\r\n",{"ruleId":"120","severity":1,"message":"121","line":29,"column":12,"nodeType":"122","messageId":"123","endLine":29,"endColumn":27},{"ruleId":"120","severity":1,"message":"124","line":30,"column":14,"nodeType":"122","messageId":"123","endLine":30,"endColumn":27},"no-unused-vars","'confirmPassword' is assigned a value but never used.","Identifier","unusedVar","'loadingUpdate' is assigned a value but never used."]